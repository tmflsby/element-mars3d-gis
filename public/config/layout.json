{
  "title": "防汛应急综合指挥平台",
  "redirect": "/baseInfo",
  "routes": [
    {
      "path": "/baseInfo",
      "name": "BaseInfo",
      "title": "综合信息",
      "panel": [
        {
          "component": "OverviewPanel",
          "visible": true,
          "style": {
            "position": "absolute",
            "top": "80px",
            "left": "20px",
            "width": "400px",
            "height": "calc(100% - 130px)"
          },
          "props": {
            "style": {},
            "subContainer": [
              {
                "title": "基本信息",
                "style": {
                  "flex": 1
                },
                "detailBtn": {
                  "show": true,
                  "title": "详情"
                },
                "component": "BaseInfo"
              },
              {
                "title": "水工程信息",
                "style": {
                  "flex": 1
                },
                "detailBtn": {
                  "show": true,
                  "title": "详情"
                },
                "component": "WaterProjectInfo"
              },
              {
                "title": "台风信息",
                "style": {
                  "flex": 1
                },
                "detailBtn": {
                  "show": false,
                  "title": "台风"
                },
                "component": "TyphoonInfo"
              }
            ]
          }
        },
        {
          "component": "OverviewPanel",
          "visible": true,
          "style": {
            "position": "absolute",
            "top": "80px",
            "right": "20px",
            "width": "400px",
            "height": "calc(100% - 130px)"
          },
          "props": {
            "style": {},
            "subContainer": [
              {
                "title": "地灾隐患点",
                "style": {
                  "flex": 1
                },
                "detailBtn": {
                  "show": true,
                  "title": "详情"
                },
                "component": "DisasterHazardPoint"
              },
              {
                "title": "应急转移",
                "style": {
                  "flex": 1
                },
                "detailBtn": {
                  "show": true,
                  "title": "详情"
                },
                "component": "EmergencyTransfer"
              },
              {
                "title": "视频监控",
                "style": {
                  "flex": 2.5
                },
                "detailBtn": {
                  "show": false,
                  "title": "详情"
                },
                "component": "VideoMonitor"
              }
            ]
          }
        }
      ]
    },
    {
      "path": "/floodSituation",
      "name": "FloodSituation",
      "title": "一屏知汛"
    },
    {
      "path": "/dispatchDecision",
      "name": "DispatchDecision",
      "title": "调度决策",
      "afterSystemTitle": true
    },
    {
      "path": "/emergencyCommand",
      "name": "EmergencyCommand",
      "title": "应急指挥"
    }
  ]
}